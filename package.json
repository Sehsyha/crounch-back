{
  "name": "grocery-back",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc --include=\"src/**/*.ts\" --report-dir=coverage --reporter=text --reporter=html --reporter=lcov mocha --require ts-node/register --recursive \"test/**/*.spec.ts\"",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "build": "rimraf dist && tsc",
    "lint": "tslint './src/**/*.ts' './test/**/*.ts'",
    "start": "node dist/src/Server.js",
    "test": "mocha --require ts-node/register test/**/*.spec.ts"
  },
  "author": "Sehsyha",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/chai-as-promised": "^7.1.0",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.1",
    "@types/express-jwt": "0.0.40",
    "@types/mocha": "^5.2.0",
    "@types/sinon": "^5.0.0",
    "@types/winston": "^2.3.9",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "sinon": "^5.0.10",
    "ts-node": "^6.0.5",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "jwks-rsa": "^1.3.0",
    "method-override": "^2.3.10",
    "winston": "^2.4.2"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "dist",
      "src/**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [],
    "all": true
  }
}
